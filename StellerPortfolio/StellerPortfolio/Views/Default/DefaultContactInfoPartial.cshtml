@model List<StellerPortfolio.Models.TblContact>

@foreach (var item in Model)
{
    <div class="contact-infos">
        <div class="item">
            <i class="ti-location-pin"></i>
            <div class="">
                <h5>Adress</h5>
                <p> @item.Addres</p>
            </div>
        </div>
        <div class="item">
            <i class="ti-mobile"></i>
            <div>
                <h5>Telefon No</h5>
                <p>@item.Phone</p>
            </div>
        </div>
        <div class="item">
            <i class="ti-email"></i>
            <div class="mb-0">
                <h5>Email Adres</h5>
                <p>@item.Email</p>
            </div>
        </div>
    </div>
}

    </div>
</div>

@foreach (var item in Model) { 
<div id="map">
    <iframe src="@item.MapUrl"></iframe>
</div>}